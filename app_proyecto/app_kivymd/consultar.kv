<MDIconButton>:
    pos_hint:{"center_y":0.5}
    size_hint_x:0.1
    theme_text_color:"Custom"
    text_color: 0,0,0,1

<Consultar@MDBoxLayout>:
    orientation:"vertical"

    MDBoxLayout:
        md_bg_color: app.theme_cls.primary_color
        size_hint_y:0.1
        MDIconButton:
            id:button
            icon: "dots-vertical"
            on_release:app.root.menu.open()
        MDLabel:
            id: toplabel
            text: "Consultar stock"
            font_style:"H5"
            halign:"center"
            theme_text_color:"Custom"
            text_color: 0,0,0,1
        MDIconButton:
            id:buttonhome
            icon: "home"
            font_size:"48sp"
            on_release:app.root.current="Home"

    MDWidget:
        size_hint_y:0.1
        md_bg_color: app.theme_cls.bg_darkest
    
    MDBoxLayout:
        size_hint_y:0.6
        MDLabel:
            text:"Acá va la tabla super x8"
            halign:"center"
            pos_hint:{"center_x":0.5,"center_y":0.5}

    MDWidget:
        size_hint_y:0.1
        md_bg_color: app.theme_cls.bg_darkest
    
    MDBoxLayout:
        size_hint_y:0.1
        MDLabel:
            text:"Acá va algo"
            halign:"center"
            pos_hint:{"center_x":0.5,"center_y":0.5}
        
        