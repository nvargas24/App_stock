<MDIconButton>:
    pos_hint:{"center_y":0.5}
    size_hint_x:0.1
    theme_text_color:"Custom"
    text_color: 0,0,0,1

<Consultar@MDBoxLayout>:
    orientation:"vertical"
    
    MDBoxLayout:
        md_bg_color: app.theme_cls.primary_color
        size_hint: 1, 1
        MDIconButton:
            id:button
            size_hint: 1, 1
            icon: "dots-vertical"
            on_release:app.root.menu.open()
        MDIconButton:
            id:full_cat
            size_hint: .5, 1
            icon: "tray-full"
            on_release:app.root.full_cat()
        MDIconButton:
            id: buscar
            size_hint: .5, 1
            icon: "magnify"
            md_bg_color: app.theme_cls.primary_color
            on_release: app.root.show_buscar()        
        MDBoxLayout:
            id: field_search
            size_hint: 8, 1
        MDIconButton:
            id:buttongraph
            size_hint: 1, 1
            icon: "image"
            font_size:"48sp"
            #on_release:app.root.current="Home"
        MDIconButton:
            id:buttonhome
            size_hint: 1, 1
            icon: "home"
            font_size:"48sp"
            on_release:app.root.current="Home"

    MDBoxLayout:
        size_hint: 1, 10
        orientation: 'vertical'
        spacing: 20
        padding: 30
        MDBoxLayout:
            size_hint: 1, 40                
            id: table
